{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "rootDir:": "./",
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "dist/*",
      "node_modules/*",
      "volumes/*"
    ]
  },
  "scripts": {
    "dev:watch": "node --watch ./src/index.ts",
    "dev": "nodemon ./src/index.ts --exec ts-node",
    "build": "rm -rf dist && tsc",
    "start": "node ./dist/index.js",
    "start:prod": "pnpm install --force && rm -rf dist && tsc && node ./dist/index.js",
    "dev:all": "concurrently 'pnpm run dev' 'cd client/ && pnpm run dev:turbo'",
    "build:all": "pnpm run build && cd client/ && pnpm run build",
    "prod": "pnpm start ./dist/index.js",
    "seed": "ts-node ./src/data/seeders/seed.ts",
    "test": "vitest"
  },
  "keywords": [
    "express",
    "typescript",
    "nodejs"
  ],
  "author": "Aurelius Ivan Wijaya",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/lib-storage": "^3.540.0",
    "@google/generative-ai": "^0.2.1",
    "@types/nodemailer": "^6.4.14",
    "amqplib": "^0.10.3",
    "aws-sdk": "^2.1588.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bull": "^4.12.2",
    "cloudinary": "^2.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "fast-jwt": "^4.0.0",
    "fastify": "^4.26.2",
    "formidable": "^3.5.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.5.0",
    "mongoose": "^8.2.4",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5",
    "ts-lint": "^4.5.1",
    "ts-node": "^10.9.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@faker-js/faker": "^8.4.1",
    "@types/amqplib": "^0.10.5",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/gulp": "^4.0.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "babel-plugin-module-resolver": "^5.0.0",
    "concurrently": "^8.2.2",
    "gulp": "^5.0.0",
    "joi": "^17.12.2",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0",
    "wrangler": "^3.41.0",
    "yargs": "^17.7.2"
  }
}
